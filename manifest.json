{
    "name": "GitHub: Sticky header",
    "version": "0.2.0",
    "manifest_version": 2,
    "description": "Makes the GitHub menu bar sticky.",
    "icons": {
        "16": "icons/icon.png",
        "48": "icons/icon.png",
        "128": "icons/icon.png"
    },
    "permissions": [
        "http://github.com/* https://github.com/* http://*.github.com/* https://*.github.com/*"
    ],
    "content_scripts": [
        {
            "matches": [
                "http://github.com/*",
                "https://github.com/*"
            ],
            "css": [
                "src/inject/inject.css"
            ]
        }
    ]
}
